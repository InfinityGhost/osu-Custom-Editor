<UserControl x:Class="osu__Custom_Editor_v2.CommandBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:osu__Custom_Editor_v2"
             mc:Ignorable="d" 
             d:DesignWidth="800" d:DesignHeight="18">
    <Grid>
        <TextBox x:Name="Box" Background="Transparent" Foreground="White" KeyDown="Box_KeyDown"/>
    </Grid>
</UserControl>
